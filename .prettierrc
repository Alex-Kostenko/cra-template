{
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "printWidth": 180,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "bracketSpacing": false,
  "arrowParens": "avoid",
